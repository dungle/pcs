    <Name>SOInvoice_SaleContract</Name>
    <DataSource>
      <ConnectionString>Provider=SQLOLEDB.1;Password=123hdvtqt;Persist Security Info=True;User ID=sa;Initial Catalog=Map;Data Source=TUANTQ</ConnectionString>
      <RecordSource>SELECT  SO_ConfirmShipMaster.ConfirmShipMasterID,
	SO_ConfirmShipMaster.InvoiceNo as ConfirmShipNo, 
	SO_ConfirmShipMaster.ShippedDate,

	SO_ConfirmShipMaster.ReferenceNo as CustomerPurchaseOrderNo,

	MST_Country.Name as LocationCountry,
	MST_Party.Name AS PartyName,
	MST_Party.Address AS PartyAddress,	
	partyCountry.Name as PartyCountry,
	MST_Currency.Code CurrencyCode,

	MST_Party.MAPBankAccountNo,
	Case when CharIndex('#', MST_Party.MAPBankAccountName) &gt; 0 then
                 Substring(MST_Party.MAPBankAccountName, 1, CharIndex('#', MST_Party.MAPBankAccountName) - 1) 
	    else MST_Party.MAPBankAccountName	
        End As MAPBankAccountName,

        Case when CharIndex('#', MST_Party.MAPBankAccountName) &gt; 0 then
                 Substring(MST_Party.MAPBankAccountName, CharIndex('#', MST_Party.MAPBankAccountName) + 1, Len(MST_Party.MAPBankAccountName))
	    else ''
        End As MAPBankAccountAddress,

	SO_ConfirmShipMaster.Comment,
	SO_ConfirmShipMaster.IssuingBank,
	SO_ConfirmShipMaster.OnBoardDate,

	SO_ConfirmShipMaster.LCNo,
	SO_ConfirmShipMaster.LCDate,

	SO_ConfirmShipMaster.CNo,
	SO_ConfirmShipMaster.VesselName,
	SO_ConfirmShipMaster.FromPort,
	shippingLoc.Description as ShippingLocationName,
	MST_PaymentTerm.Description as PaymentTerm,		
	MST_PartyContact.Name as PartyContactName,
	MST_PartyContact.Phone as PartyContactPhone,
	MST_PartyContact.Fax as PartyContactFax,	
	MST_Carrier.Name as CarrierName,

	(SELECT SUM(detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	 ) as TotalQuantity,
	
	(SELECT SUM( detail.Price * detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	 ) as TotalAmount,

	(
	 (SELECT SUM( detail.Price * detail.InvoiceQty)
	  FROM SO_ConfirmShipDetail detail
	  WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	  AND detail.ProductID = ITM_Product.ProductID
	  )	
	  /
	 (
	 Case
	  When(
	     SELECT SUM(detail.InvoiceQty)
	     FROM SO_ConfirmShipDetail detail
	     WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	     AND detail.ProductID = ITM_Product.ProductID
	   ) = 0 
	   then 1
	   else   
  	     (
	      SELECT SUM(detail.InvoiceQty)
	      FROM SO_ConfirmShipDetail detail
	      WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	      AND detail.ProductID = ITM_Product.ProductID
	      )
	    end      
	 )
	)
	as AVGPrice,


	(
	(SELECT SUM( detail.Price * detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	 AND detail.ProductID = ITM_Product.ProductID
	 )	
	/
	(SELECT SUM(detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	 AND detail.ProductID = ITM_Product.ProductID
	 )	
	)
	 as AVGPrice,
	
	ITM_Product.ProductID,
	SO_ConfirmShipDetail.ConfirmShipDetailID,
	SO_CustomerItemRefDetail.CustomerItemCode as PartNo,
	ITM_Product.Description as PartName,
	ITM_Product.Revision as PartModel,
	MST_UnitOfMeasure.Code AS MST_UnitOfMeasureCode,
	ITM_Category.Name as CategoryName,

	ITM_Product.QuantitySet,
	
	(SELECT SUM(detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.SaleOrderDetailID = SO_SaleOrderDetail.SaleOrderDetailID
	       AND detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	       AND detail.ConfirmShipDetailID = SO_ConfirmShipDetail.ConfirmShipDetailID 
	) as ShippingQuantity,

	SO_ConfirmShipDetail.Price as UnitPrice,

	(SELECT SUM(detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
	 ) * SO_ConfirmShipDetail.Price AS NetAmount

FROM SO_ConfirmShipDetail
	INNER JOIN SO_ConfirmShipMaster ON SO_ConfirmShipDetail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
     	INNER JOIN SO_SaleOrderMaster ON SO_ConfirmShipMaster.SaleOrderMasterID = SO_SaleOrderMaster.SaleOrderMasterID
     	INNER JOIN SO_SaleOrderDetail ON SO_ConfirmShipDetail.SaleOrderDetailID = SO_SaleOrderDetail.SaleOrderDetailID 
     	INNER JOIN ITM_Product ON ITM_Product.ProductID = SO_SaleOrderDetail.ProductID
     	INNER JOIN MST_UnitOfMeasure ON SO_SaleOrderDetail.SellingUMID = MST_UnitOfMeasure.UnitOfMeasureID
     	INNER JOIN MST_Currency ON SO_ConfirmShipMaster.CurrencyID = MST_Currency.CurrencyID
     	INNER JOIN MST_Party ON SO_SaleOrderMaster.PartyID = MST_Party.PartyID
	
	LEFT JOIN MST_PartyContact ON SO_SaleOrderMaster.PartyContactID = MST_PartyContact.PartyContactID
	LEFT JOIN MST_Country partyCountry ON partyCountry.CountryID = MST_Party.CountryID
	LEFT JOIN MST_PaymentTerm ON MST_Party.PaymentTermID = MST_PaymentTerm.PaymentTermID
	LEFT JOIN MST_PartyLocation shippingLoc ON shippingLoc.PartyLocationID = SO_SaleOrderMaster.ShipToLocID
	LEFT JOIN MST_Country ON shippingLoc.CountryID = MST_Country.CountryID
	LEFT JOIN ITM_Category ON ITM_Category.CategoryID = ITM_Product.CategoryID
	LEFT JOIN MST_Carrier ON MST_Carrier.CarrierID = SO_SaleOrderMaster.CarrierID

	LEFT JOIN SO_CustomerItemRefMaster ON SO_CustomerItemRefMaster.PartyID = MST_Party.PartyID
	LEFT JOIN SO_CustomerItemRefDetail ON SO_CustomerItemRefMaster.CustomerItemRefMasterID = SO_CustomerItemRefDetail.CustomerItemRefMasterID
		AND SO_CustomerItemRefDetail.ProductID = ITM_Product.ProductID

WHERE SO_ConfirmShipMaster.ConfirmShipNo = 'SS1406060002'	
	AND 
	(SELECT SUM(detail.InvoiceQty)
	 FROM SO_ConfirmShipDetail detail
	 WHERE detail.ConfirmShipMasterID = SO_ConfirmShipMaster.ConfirmShipMasterID
     ) &gt; 0

ORDER BY SO_ConfirmShipMaster.ConfirmShipMasterID, 
	 ITM_Product.Revision,
	 ITM_Product.Description
</RecordSource>
